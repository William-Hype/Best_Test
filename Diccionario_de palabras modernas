meme_dict = {
  "CRINGE": "pena agena"
  "LOL": "Una respuesta a algo gracioso"
  "ROFL": "una respuesta a una broma"

  word = input("Escribe una palabra modernas que no entiendes")

  if word in meme_dict.keys():
    print(meme_dict[word])
else:
    print("Todav√≠a no tenemos esta palabra... Pero estamos trabajando en ella.")
